{
    "name": "thrubus/wp-thrubus-post-type",
    "description": "WordPress Thrubus Post Type Plugin",
    "version": "1.0.1",
    "type": "library",
    "license": "Mit",
    "authors": [
        {
            "name": "vigih dev",
            "email": "vigihdev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "WPPostType\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WPPostType\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "server-example": "php -S localhost:2378 examples/index.php",
        "server": "php -S localhost:2377 server.php"
    },
    "config": {
        "process-timeout": 0
    }
}